From af84b8790ecbc392e88e4335fe3076d5c1d4dffa Mon Sep 17 00:00:00 2001
From: arathinx <ayyappanx.rathinam@intel.com>
Date: Mon, 22 Jul 2019 11:54:33 +0800
Subject: [PATCH] DRVLIB_SW-2619/DRVLIB_SW-1088: replace falconmx with prx300

---
 drivers/char/ltq_mps2/drv_mps_linux.c | 2 +-
 drivers/pinctrl/Kconfig               | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/drivers/char/ltq_mps2/drv_mps_linux.c b/drivers/char/ltq_mps2/drv_mps_linux.c
index 1bb39a9ee4c5..41307cd275a9 100644
--- a/drivers/char/ltq_mps2/drv_mps_linux.c
+++ b/drivers/char/ltq_mps2/drv_mps_linux.c
@@ -874,7 +874,7 @@ static void mps_proc_entrycreate ( struct proc_dir_entry *parent_node,
    Example entry of needed device tree nodes.
    \verbatim
    mps@107000 {
-      compatible = "lantiq,mps2-falconmx","lantiq,mps2-xrx500";
+      compatible = "lantiq,prx300-mps2","lantiq,mps2-xrx500";
       reg = <0x107000 0x400>;
       interrupt-parent = <&icu0>;
       interrupts = <154 155>;
diff --git a/drivers/pinctrl/Kconfig b/drivers/pinctrl/Kconfig
index 9478710d9ee5..17c86e1b824e 100755
--- a/drivers/pinctrl/Kconfig
+++ b/drivers/pinctrl/Kconfig
@@ -293,7 +293,7 @@ config PINCTRL_TB10X
 	select GPIOLIB
 
 config PINCTRL_EQUILIBRIUM
-	tristate "Intel xrx500/falconmx pinctrl and GPIO driver"
+	tristate "Intel xrx500/prx300 pinctrl and GPIO driver"
 	select PINMUX
 	select PINCONF
 	select GPIOLIB
